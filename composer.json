{
    "name": "nztim/mailchimp",
    "description": "Mailchimp API wrapper, includes Laravel 5 support.",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim",
            "email": "tim@nztim.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "nztim/simplehttp": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nztim/simplehttp"
        }
    ],
    "autoload": {
        "psr-4": {
            "NZTim\\Mailchimp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NZTim\\Mailchimp\\MailchimpServiceProvider"
            ],
            "aliases": {
                "Mailchimp": "NZTim\\Mailchimp\\MailchimpFacade"
            }
        }
    }
}
